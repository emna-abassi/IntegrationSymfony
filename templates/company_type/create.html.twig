{% extends "base.html.twig"%}

{% block body %}

<div class="container">
<div class="row">
<h3 class="mt-5 mb-5"> Create Company </h3>
{%for message in app.flashes('notice')%}
<div class="alert alert-success">
{{message}}
</div>
{%endfor%}
<div class="col-md-6 offset-md-3">
{{ form_start(form) }}
<div class="form-group">
{{form_label(form.Name)}}
{{form_widget(form.Name,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.adress)}}
{{form_widget(form.adress,{'attr':{'class':'form-control'}})}}
</div>





<div class="form-group">
{{form_label(form.ville)}}
{{form_widget(form.ville,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.phone)}}
{{form_widget(form.phone,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.zone)}}
{{form_widget(form.zone,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.rib)}}
{{form_widget(form.rib,{'attr':{'class':'form-control'}})}}
</div>



<div class="form-group">
{{form_label(form.code_postal)}}
{{form_widget(form.code_postal,{'attr':{'class':'form-control'}})}}
</div>


<div class="form-group">
{{form_label(form.numberSiren)}}
{{form_widget(form.numberSiren,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.numberTVA)}}
{{form_widget(form.numberTVA,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.juridicForm)}}
{{form_widget(form.juridicForm,{'attr':{'class':'form-control'}})}}
</div>

<div class="form-group">
{{form_label(form.representantLegal)}}
{{form_widget(form.representantLegal,{'attr':{'class':'form-control'}})}}
</div>





<div class="form-group">
<button class="btn btn-info mt-2 text-white">submit </button>
</div>

{{form_end(form)}}

</div>
</div>
</div>
{%endblock%}
